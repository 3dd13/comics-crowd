<h1>Listing comic_requests</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Author</th>
    <th>Book cover image url</th>
    <th>Online reading website url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @comic_requests.each do |comic_request| %>
  <tr>
    <td><%= comic_request.name %></td>
    <td><%= comic_request.author %></td>
    <td><%= comic_request.book_cover_image_url %></td>
    <td><%= comic_request.online_reading_website_url %></td>
    <td><%= link_to 'Show', comic_request %></td>
    <td><%= link_to 'Edit', edit_comic_request_path(comic_request) %></td>
    <td><%= link_to 'Destroy', comic_request, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Comic request', new_comic_request_path %>
